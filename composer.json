{
  "name": "nandobas/php_composer",
  "description": "",
  "type": "project",
  "authors": [
    {
      "name": "Fernando basilio",
      "email": "fernando.basilio@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "psr/http-message": "^1.0",
    "guzzlehttp/guzzle": "^7.3",
    "nyholm/psr7": "^1.4",
    "nyholm/psr7-server": "^1.0",
    "nikic/fast-route": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src\\"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests\\"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
